@model K309Portfolio.Models.PortfolioSection
@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/AdminPanel/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Details for @Model.header</h2>
<dl class="dl-horizontal">
    <dt>Header</dt>
    <dd>@Model.header</dd>
    <dt>Description</dt>
    <dd>@Model.deccription</dd>

    <dt>Image</dt>
    <dd>
        <img width="200" class="img-thumbnail" src="@Model.imageUrl" />
    </dd>
    <dt>Publish Date</dt>
    <dd>
        @Model.publishDate
    </dd>

    <dt>
        <a href="@Url.Action("Edit",new { id=Model.id})"><i class="icon-note"></i>  Edit</a> |
        <a href="@Url.Action("Index")">Come Back</a>

    </dt>
</dl>

